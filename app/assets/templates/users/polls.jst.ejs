<div class="text-center">
  <h1>Your polls</h1>

  <ul style="list-style: none;" class="polls">
    <% polls.forEach(function (poll, index) { %>
      <li class="poll">
        <a href="#/polls/<%= poll.id %>"><h3><%= poll.title %></h3></a>
        <br>
      </li>
    <% }); %>
  </ul>

  <% if (!polls.toString()) { %>
    <p>None yet. Let's get started!</p>
  <% } %>
  <br>

  <a href="#/polls/new" class="btn btn-warning">Create new poll</a>
</div>
